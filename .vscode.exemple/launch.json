  "version": "0.2.0",
  "justMyCode": false,
  "configurations": [
    {
      "name": "Shop_Back: Project",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "0.0.0.0:8000"
      ],
      "django": true,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env"

    },
    {
      "name": "Shop_Back: Shell",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runscripts" // shell_plus, runscripts 
      ],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Shop_Back: collectstatic",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "collectstatic"
      ],
      "django": true
    },
    {
      "name": "Shop_Back: makemigrations",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "makemigrations"
      ],
      "django": true
    },
    {
      "name": "Shop_Back: migrate",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "migrate"
      ],
      "django": true
    },
    {
      "name": "Shop_Back: Super User",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "createsuperuser"
